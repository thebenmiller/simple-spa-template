{
  "name": "simple-spa",
  "version": "0.1.0",
  "description": "Basic webpack SPA",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV='production' webpack -p --progress --colors --config lib/webpack.config.js",
    "start": "webpack-dev-server --open --progress --colors --config lib/webpack.config.js",
    "dev": "webpack-dashboard -m -- webpack-dev-server --open --progress --colors --config lib/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thebenmiller/simple-spa-template.git"
  },
  "author": "Ben Miller <thebenmiller@gmail.com>",
  "license": "The Unlicense",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.6.1",
    "eslint-config-base": "^1.0.0",
    "eslint-plugin-import": "^2.7.0",
    "html-webpack-plugin": "^2.30.1",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.7.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "a-big-triangle": "^1.0.3",
    "babel-plugin-glslify": "^2.0.0",
    "gl-shader": "^4.2.1",
    "glslify": "^6.1.0",
    "glslify-loader": "^1.0.2",
    "raw-loader": "^0.5.1"
  }
}
